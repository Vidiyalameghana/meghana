n=int(input())
d={}
for i in range(n):
    x=input().split()
    d[x[0]]=x[1]
while True:
    try:
        EMAIL=input()
        if EMAIL in d:
            print(EMAIL,"=",d[EMAIL],sep='')
        else:print("not found")
    except:
        break

